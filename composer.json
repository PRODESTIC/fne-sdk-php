{
  "name": "prodestic/fne-sdk-php",
  "description": "SDK PHP pour l'API de Facturation Normalisée Électronique (FNE) de la DGI Côte d'Ivoire",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "type": "library",
  "keywords": ["fne", "dgi", "cote-ivoire", "facturation", "api"],
  "authors": [
    {
      "name": "PRODESITC SARL",
      "email": "a.niando@prodestic.net"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "psalm/plugin-phpunit": "^0.16.1",
    "vimeo/psalm": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "DgiCi\\FneSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DgiCi\\FneSdk\\Tests\\": "tests/"
    }
  }
}